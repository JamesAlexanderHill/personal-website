{
  "name": "personal-website",
  "version": "3.1.0",
  "description": "A personal website",
  "devDependencies": {
    "@mischnic/parcel-namer-preserve-structure": "^0.2.0",
    "@parcel/packager-raw-url": "^2.6.2",
    "@parcel/packager-xml": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@parcel/transformer-webmanifest": "^2.6.2",
    "@parcel/transformer-xml": "^2.6.2",
    "eslint": "^8.21.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.6.2",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "postcss": "^8.4.14",
    "posthtml": "^0.16.6",
    "posthtml-expressions": "^1.9.0",
    "posthtml-include": "^1.7.4",
    "posthtml-inline-svg": "^0.2.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "tailwindcss": "^3.1.7",
    "tailwindcss-animation-delay": "^1.0.7"
  },
  "scripts": {
    "watch": "parcel ./src/*.html",
    "lint": "run-p lint:*",
    "lint-fix": "run-p lint-fix:*",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint-fix:css": "stylelint 'src/**/*.css' --fix",
    "lint:js": "eslint 'src/**/*.js'",
    "lint-fix:js": "eslint 'src/**/*.js' --fix",
    "build": "parcel build src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesAlexanderHill/personal-website.git"
  },
  "author": "James Hill",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JamesAlexanderHill/personal-website/issues"
  },
  "homepage": "https://github.com/JamesAlexanderHill/personal-website#readme"
}
